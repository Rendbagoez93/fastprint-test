# Django Settings
# For local development: config.settings.local (uses SQLite)
# For production: config.settings.production (uses PostgreSQL)
DJANGO_SETTINGS_MODULE=config.settings.local

SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Settings (PostgreSQL)
# Only required for production (config.settings.production)
# Local development uses SQLite automatically
DB_NAME=fastprint
DB_USER=postgres
DB_PASSWORD=your-secure-database-password
DB_HOST=localhost
DB_PORT=5432
DB_CONN_MAX_AGE=600

# Production Security Settings (optional)
# SECURE_SSL_REDIRECT=True
